{
  "name": "Ambire Wallet",
  "expo": {
    "slug": "ambire-mobile-wallet",
    "version": "1.0.4",
    "assetBundlePatterns": ["**/*"],
    "runtimeVersion": "2",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 45000
    },
    "ios": {
      "bundleIdentifier": "com.ambire.wallet"
    },
    "plugins": ["sentry-expo"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "your sentry organization's short name here",
            "project": "your sentry project's name here",
            "authToken": "your auth token here"
          }
        }
      ]
    }
  }
}
