{
  "cli": {
    "version": ">= 0.39.0"
  },
  "build": {
    "production": {
      "node": "16.13.0",
      "ios": {
        "simulator": true
      },
      "releaseChannel": "default"
    },
    "staging": {
      "extends": "production",
      "distribution": "internal",
      "releaseChannel": "staging"
    },
    "development": {
      "extends": "production",
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "releaseChannel": "development"
    }
  },
  "submit": {
    "production": {}
  }
}
