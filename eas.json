{
  "cli": {
    "version": ">= 0.39.0"
  },
  "build": {
    "production": {
      "node": "16.13.0",
      "ios": {
        "credentialsSource": "local"
      },
      "android": {
        "credentialsSource": "local"
      },
      "releaseChannel": "default",
      "env": {
        "APP_ENV": "production"
      }
    },
    "staging": {
      "extends": "production",
      "distribution": "internal",
      "releaseChannel": "staging",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "development": {
      "extends": "production",
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "releaseChannel": "development",
      "env": {
        "APP_ENV": "development"
      }
    },
    "production:simulator": {
      "extends": "production",
      "ios": {
        "simulator": true
      }
    },
    "staging:simulator": {
      "extends": "staging",
      "ios": {
        "simulator": true
      }
    },
    "development:simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
