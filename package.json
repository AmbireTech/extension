{
  "name": "ambire-mobile-wallet",
  "main": "index.js",
  "scripts": {
    "android": "EAS_BUILD_PLATFORM=android bash ./scripts/expo-build-post-install.sh && expo run:android",
    "android:staging": "APP_ENV=staging yarn android",
    "android:production": "APP_ENV=production yarn android",
    "ios": "EAS_BUILD_PLATFORM=ios bash ./scripts/expo-build-post-install.sh && expo run:ios",
    "ios:staging": "APP_ENV=staging yarn ios",
    "ios:production": "APP_ENV=production yarn ios",
    "web:webkit": "rm -rf ./webkit-dev && WEB_ENGINE=webkit WEBPACK_BUILD_OUTPUT_PATH=webkit-dev APP_ENV=development expo start --web",
    "web:gecko": "rm -rf ./gecko-dev && WEB_ENGINE=gecko WEBPACK_BUILD_OUTPUT_PATH=gecko-dev APP_ENV=development expo start --web",
    "start": "react-native start",
    "start:clean": "react-native start --reset-cache",
    "start:staging": "APP_ENV=staging yarn start",
    "start:clean:staging": "APP_ENV=staging yarn start:clean",
    "start:production": "APP_ENV=production yarn start",
    "start:clean:production": "APP_ENV=production yarn start:clean",
    "eas-build-pre-install": "./scripts/expo-build-pre-install.sh",
    "eas-build-post-install": "./scripts/expo-build-post-install.sh",
    "build:configure": "eas build:configure",
    "build:android:development": "eas build --platform android --profile development",
    "build:android:staging": "eas build --platform android --profile staging",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:development": "eas build --platform ios --profile development",
    "build:ios:staging": "eas build --platform ios --profile staging",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:ios:production:simulator": "eas build --platform ios --profile production:simulator",
    "build:android:production:simulator": "eas build --platform android --profile production:simulator",
    "build:web:webkit": "rm -rf ./webkit-prod && APP_ENV=production WEB_ENGINE=webkit WEBPACK_BUILD_OUTPUT_PATH=webkit-prod expo build:web",
    "build:web:gecko": "rm -rf ./gecko-prod && APP_ENV=production WEB_ENGINE=gecko WEBPACK_BUILD_OUTPUT_PATH=gecko-prod expo build:web",
    "publish:development": "eas update --channel development",
    "publish:staging": "APP_ENV=staging eas update --channel staging",
    "publish:production": "APP_ENV=production eas update --channel default",
    "translations:scan": "i18next-scanner --config ./scripts/i18next-scanner.config.js './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint ./src/** --ext .js,.jsx,.ts,.tsx --fix",
    "expo:doctor": "expo doctor",
    "expo:upgrade": "expo upgrade",
    "expo:install": "expo install",
    "eas:secret:list": "eas secret:list",
    "eas:secret:push": "eas secret:push --scope project --env-file .env",
    "test:e2e:web": "jest --config=./src/web/jest.config.js"
  },
