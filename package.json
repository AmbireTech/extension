{
  "name": "ambire-mobile-wallet",
  "main": "index.js",
  "scripts": {
    "add:web": "MODE=WEB yarn add",
    "remove:web": "MODE=WEB yarn remove",
    "install:web": "rm -rf ./node_modules && MODE=WEB yarn install",
    "android": "expo run:android",
    "android:staging": "APP_ENV=staging yarn android",
    "android:production": "APP_ENV=production yarn android",
    "ios": "expo run:ios",
    "ios:staging": "APP_ENV=staging yarn ios",
    "ios:production": "APP_ENV=production yarn ios",
    "web:webkit": "rm -rf ./webkit-dev && WEB_ENGINE=webkit WEBPACK_BUILD_OUTPUT_PATH=webkit-dev expo start --web",
    "web:gecko": "rm -rf ./gecko-dev && WEB_ENGINE=gecko WEBPACK_BUILD_OUTPUT_PATH=gecko-dev expo start --web",
    "start": "react-native start",
    "postinstall": "node ./scripts/postinstall.js",
    "start:clean": "react-native start --reset-cache",
    "start:staging": "APP_ENV=staging yarn start",
    "start:clean:staging": "APP_ENV=staging yarn start:clean",
    "start:production": "APP_ENV=production yarn start",
    "start:clean:production": "APP_ENV=production yarn start:clean",
    "build:configure": "eas build:configure",
    "build:android:development": "eas build -p android --profile development",
    "build:android:staging": "eas build -p android --profile staging",
    "build:android:production": "eas build -p android --profile production",
    "build:ios:development": "eas build -p ios --profile development",
    "build:ios:staging": "eas build -p ios --profile staging",
    "build:ios:production": "eas build -p ios --profile production",
    "build:ios:production:simulator": "eas build -p ios --profile production:simulator",
    "build:android:production:simulator": "eas build -p android --profile production:simulator",
    "build:web:webkit": "rm -rf ./webkit-prod && APP_ENV=production WEB_ENGINE=webkit WEBPACK_BUILD_OUTPUT_PATH=webkit-prod expo build:web",
    "build:web:gecko": "rm -rf ./gecko-prod && APP_ENV=production WEB_ENGINE=gecko WEBPACK_BUILD_OUTPUT_PATH=gecko-prod expo build:web",
    "publish:development": "expo publish --release-channel development",
    "publish:staging": "APP_ENV=staging expo publish --release-channel staging",
    "publish:production": "APP_ENV=production expo publish --release-channel default",
    "translations:scan": "i18next-scanner --config ./scripts/i18next-scanner.config.js './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint ./src/** --ext .js,.jsx,.ts,.tsx --fix",
    "expo:doctor": "expo doctor",
    "expo:upgrade": "expo upgrade",
    "eas:secret:list": "eas secret:list",
    "eas:secret:push": "eas secret:push --scope project --env-file .env"
  },
