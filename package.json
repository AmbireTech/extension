    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "expo": "~43.0.2",
    "expo-asset": "~8.4.3",
    "expo-localization": "~11.0.0",
    "expo-splash-screen": "~0.13.5",
    "expo-status-bar": "~1.1.0",
    "expo-updates": "~0.10.15",
