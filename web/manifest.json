{
  "name": "Ambire Wallet",
  "description": "The Web3 wallet that speaks human",
  "version": "1.0",
  "manifest_version": 3,
  "short_name": "Ambire",
  "permissions": ["storage", "tabs", "notifications"],
  "icons": {
    "128": "./assets/images/extension_logo.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["https://*/"],
  "externally_connectable": {
    "ids": ["*"]
  }
}
