{
  "name": "e2e-playwright-test",
  "version": "1.0.0",
  "description": "Playwright automation",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "playwright test --grep-invert @balanceCheck",
    "show:trace": "playwright show-trace",
    "balance:test": "npx playwright test -g @balanceCheck",
    "auth": "npx playwright test -g @auth",
    "dashboard": "npx playwright test -g @dashboard",
    "gas:tank": "npx playwright test -g @gasTank",
    "settings:add:account": "npx playwright test -g @addAccount",
    "settings:add:token": "npx playwright test -g @addToken",
    "settings:keystore": "npx playwright test -g @keystore",
    "settings:network:management": "npx playwright test -g @networkManagement",
    "sign:message": "npx playwright test -g @signMessage",
    "stability": "npx playwright test -g @stability",
    "swap:and:bridge": "npx playwright test -g @swapAndBridge",
    "transfer": "npx playwright test -g @transfer",
    "postinstall": "patch-package"
  },
  "author": "Filip",
  "devDependencies": {
    "@types/node": "^20.16.1",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@playwright/test": "^1.52.0",
    "@trezor/connect-plugin-ethereum": "^9.0.5",
    "@trezor/connect-webextension": "^9.6.1",
    "@trezor/trezor-user-env-link": "^1.0.0",
    "dotenv": "^16.4.5",
    "qs": "^6.12.0"
  }
}
